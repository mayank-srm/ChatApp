<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saksham.chatapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_mood_black_24dp"
        android:label="CHAT APP"
        android:roundIcon="@drawable/ic_mood_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".LoginScreen" />
        <activity android:name=".chat_activity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".UsersActivity" />
        <activity android:name=".StatusActivity"></activity>
    </application>

</manifest>